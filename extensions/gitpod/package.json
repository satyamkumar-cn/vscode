{
  "name": "gitpod",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "gitpod",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "enableProposedApi": true,
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*",
    "onAuthenticationRequest:gitpod",
    "onAuthenticationRequest:github",
    "onCommand:gitpod.api.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitpod.stop.ws",
        "title": "%stopWorkspace%"
      },
      {
        "command": "gitpod.open.settings",
        "title": "%openSettings%"
      },
      {
        "command": "gitpod.open.accessControl",
        "title": "%openAccessControl%"
      },
      {
        "command": "gitpod.open.context",
        "title": "%openContext%"
      },
      {
        "command": "gitpod.open.dashboard",
        "title": "%openDashboard%"
      },
      {
        "command": "gitpod.open.documentation",
        "title": "%openDocumentation%"
      },
      {
        "command": "gitpod.open.follow",
        "title": "%openFollow%"
      },
      {
        "command": "gitpod.open.community",
        "title": "%openCommunity%"
      },
      {
        "command": "gitpod.reportIssue",
        "title": "%reportIssue%"
      },
      {
        "command": "gitpod.upgradeSubscription",
        "title": "%upgradeSubscription%"
      },
      {
        "command": "gitpod.ExtendTimeout",
        "title": "%extendTimeout%"
      },
      {
        "command": "gitpod.takeSnapshot",
        "title": "%takeSnapshot%"
      },
      {
        "command": "gitpod.ports.openBrowser",
        "title": "%openBrowser%",
        "icon": "$(globe)"
      },
      {
        "command": "gitpod.ports.preview",
        "title": "%openPreview%",
        "icon": "$(open-preview)"
      },
      {
        "command": "gitpod.ports.makePrivate",
        "title": "%makePrivate%",
        "icon": "$(lock)"
      },
      {
        "command": "gitpod.ports.makePublic",
        "title": "%makePublic%",
        "icon": "$(unlock)"
      },
      {
        "command": "gitpod.extensions.addToConfig",
        "title": "%addToConfig%"
      }
    ],
    "menus": {
      "extension/context": [
        {
          "command": "gitpod.extensions.addToConfig",
          "group": "2_configure"
        }
      ],
      "menuBar/home": [
        {
          "command": "gitpod.open.context",
          "group": "gitpod@10"
        },
        {
          "command": "gitpod.open.documentation",
          "group": "gitpod@20"
        },
        {
          "command": "gitpod.takeSnapshot",
          "group": "gitpod@30"
        },
        {
          "command": "gitpod.stop.ws",
          "group": "gitpod@40"
        }
      ],
      "accounts/context": [
        {
          "command": "gitpod.open.settings",
          "group": "navigation@10"
        },
        {
          "command": "gitpod.open.accessControl",
          "group": "navigation@20"
        },
        {
          "command": "gitpod.upgradeSubscription",
          "group": "navigation@30"
        },
        {
          "command": "gitpod.ExtendTimeout",
          "group": "navigation@50"
        }
      ],
      "menuBar/help": [
        {
          "command": "gitpod.open.follow",
          "group": "z_about2@10"
        },
        {
          "command": "gitpod.open.community",
          "group": "z_about2@20"
        },
        {
          "command": "gitpod.reportIssue",
          "group": "z_about2@30"
        }
      ],
      "statusBar/windowIndicator": [
        {
          "command": "gitpod.stop.ws",
          "group": "navigation@10"
        },
        {
          "command": "gitpod.open.settings",
          "group": "navigation@20"
        },
        {
          "command": "gitpod.open.accessControl",
          "group": "navigation@30"
        },
        {
          "command": "gitpod.open.context",
          "group": "navigation@40"
        },
        {
          "command": "gitpod.open.dashboard",
          "group": "navigation@50"
        },
        {
          "command": "gitpod.open.documentation",
          "group": "navigation@60"
        },
        {
          "command": "gitpod.open.follow",
          "group": "navigation@70"
        },
        {
          "command": "gitpod.open.community",
          "group": "navigation@80"
        },
        {
          "command": "gitpod.reportIssue",
          "group": "navigation@90"
        },
        {
          "command": "gitpod.upgradeSubscription",
          "group": "navigation@100"
        },
        {
          "command": "gitpod.ExtendTimeout",
          "group": "navigation@110"
        },
        {
          "command": "gitpod.takeSnapshot",
          "group": "navigation@120"
        }
      ],
      "view/item/context": [
        {
          "command": "gitpod.ports.preview",
          "when": "view == gitpod.workspace && viewItem =~ /exposed-served-port$/",
          "group": "inline"
        },
        {
          "command": "gitpod.ports.openBrowser",
          "when": "view == gitpod.workspace && viewItem =~ /exposed-served-port$/",
          "group": "inline"
        },
        {
          "command": "gitpod.ports.makePublic",
          "when": "view == gitpod.workspace && viewItem =~ /^private/",
          "group": "inline"
        },
        {
          "command": "gitpod.ports.makePrivate",
          "when": "view == gitpod.workspace && viewItem =~ /^public/",
          "group": "inline"
        }
      ],
      "commandPalette": [
        {
          "command": "gitpod.ports.preview",
          "when": "false"
        },
        {
          "command": "gitpod.ports.openBrowser",
          "when": "false"
        },
        {
          "command": "gitpod.ports.makePublic",
          "when": "false"
        },
        {
          "command": "gitpod.ports.makePrivate",
          "when": "false"
        }
      ]
    },
    "views": {
      "remote": [
        {
          "id": "gitpod.workspace",
          "name": "DevSpaces Workspace"
        }
      ]
    },
    "authentication": [
      {
        "id": "gitpod",
        "label": "Gitpod"
      },
      {
        "id": "github",
        "label": "GitHub"
      }
    ]
  },
  "scripts": {
    "compile": "gulp compile-extension:gitpod",
    "watch": "gulp watch-extension:gitpod",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.5.8",
    "@types/ps-tree": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.6"
  },
  "dependencies": {
    "@gitpod/gitpod-protocol": "main",
    "@gitpod/supervisor-api-grpc": "main",
    "@grpc/grpc-js": "latest",
    "bufferutil": "^4.0.1",
    "node-fetch": "^2.6.1",
    "reconnecting-websocket": "^4.4.0",
    "utf-8-validate": "^5.0.2",
    "uuid": "^8.3.1",
    "vscode-nls": "^5.0.0",
    "ws": "^7.3.1",
    "yaml": "^1.10.0"
  }
}
